<div>
  <%= form_with url: "/gyms/#{@gym.id}/members", method: :post, local: true do |form| %>
      <%= form.label "New Member" %><br>
      <%= form.label "First Name" %>
      <%= form.text_field :first_name %><br>
      <%= form.label "Last Name" %>
      <%= form.text_field :last_name %><br>
      <%= form.label "Address" %>
      <%= form.text_field :address %><br>
      <%= form.label "Zip Code" %>
      <%= form.text_field :zipcode %><br>
      <%= form.label "Phone" %>
      <%= form.text_field :phone %><br>
      <%= form.label "Dues Current" %>
      <%= form.text_field :dues_current %><br>
      <%= form.submit 'Create Member' %>
  <% end %>
</div>
