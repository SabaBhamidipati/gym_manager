 <%= form_with url: '/gyms', method: :post, local: true do |form| %>
    <%= form.label :name %>
    <%= form.text_field :name %>
    <%= form.text_field :zip_code %>
    <%= form.text_field :member_cost %>
    <%= form.text_field :member_initiation_fee %>
    <%= form.text_field :guest_cost %>
    <%= form.text_field :open %>
    <%= form.submit 'Create Gym' %>
<% end %>
